        "# Importar bibliotecas necessárias\n",
        "from pyspark.sql import SparkSession\n",
        "from pyspark.sql.functions import col\n",
        "from pyspark.sql.types import StructType, StringType, DoubleType, IntegerType\n",
        "\n",
        "# Criando uma sessão Spark\n",
        "spark = SparkSession.builder \\\n",
        "    .appName(\"TransacoesRealTime\") \\\n",
        "    .getOrCreate()\n",
        "\n",
        "# Definindo o schema dos dados\n",
        "schema = StructType() \\\n",
        "    .add(\"TransactionNo\", StringType()) \\\n",
        "    .add(\"Date\", StringType()) \\\n",
        "    .add(\"ProductNo\", StringType()) \\\n",
        "    .add(\"ProductName\", StringType()) \\\n",
        "    .add(\"Price\", DoubleType()) \\\n",
        "    .add(\"Quantity\", IntegerType()) \\\n",
        "    .add(\"CustomerNo\", StringType()) \\\n",
        "    .add(\"Country\", StringType())\n",
        "\n",
        "# Simulando um stream de dados (neste caso, seria conectado ao Event Hubs/Kafka)\n",
        "df_stream = spark.readStream \\\n",
        "    .schema(schema) \\\n",
        "    .csv(\"/content/Sales Transaction v.4a.csv\")  # Caminho do CSV no Colab\n",
        "\n",
        "# Processamento de dados: Cálculo do total por transação\n",
        "df_stream = df_stream.withColumn(\"Total\", col(\"Price\") * col(\"Quantity\"))\n",
        "\n",
        "# Salvando os dados no Delta Lake\n",
        "df_stream.writeStream \\\n",
        "    .format(\"delta\") \\\n",
        "    .option(\"checkpointLocation\", \"/mnt/delta/checkpoints/transacoes\") \\\n",
        "    .start(\"/mnt/delta/transacoes_bronze\")\n",
        "\n",
        "spark.streams.awaitAnyTermination()\n"
